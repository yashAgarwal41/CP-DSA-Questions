#include<bits/stdc++.h>
#define int long long
using namespace std;

void code()
{
    int n,m; cin>>n>>m;
    int x,y,a,b; cin>>x>>y; cin>>a>>b;

    int thief=0,police=0;

    thief=(n-x) + (m-y);    //total no of steps thief take to reach last cell of prison...
    police=(n-a)+(m-b)-min(n-a,m-b);    //total no of steps thief take to reach last cell of prison...

    // if thief take less steps than police to reach last cell, he will escape...
    if(thief<=police)
        cout<<"YES"<<endl;

    // if police reach before than thief in the last cell, then they will catch him...
    else 
        cout<<"NO"<<endl;

}

// Main Function
signed main()
{
    cin.tie(NULL);
    cout.tie(NULL);
    int testcase;
    cin>>testcase;
    while(testcase--)
        code();
    
    return 0;
}
